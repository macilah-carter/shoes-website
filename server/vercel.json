   {
  "version": 2,
  "builds": [
    {
      "src": "./app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://shoes-website-backend.vercel.app/$1"
    }
  ]
}
